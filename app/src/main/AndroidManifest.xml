<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tiagocardoso.eventool">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Criar_usuarioActivity" />
        <activity
            android:name=".Listar_usuarioActivity"
            android:label="@string/app_name"
            android:parentActivityName=".Criar_usuarioActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tiagocardoso.eventool.Criar_usuarioActivity" />
        </activity>
        <activity android:name=".Listar_EventoActivity" />
        <activity android:name=".Listar_evento2Activity"></activity>
    </application>

</manifest>